{
  "name": "try1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && echo 'Build completed successfully'",
    "start": "next start",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "docker:build": "docker build -t try1-app .",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v --remove-orphans",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d --build",
    "docker:prod:down": "docker-compose -f docker-compose.prod.yml down",
    "docker:health": "docker-compose ps",
    "docker:backup": "docker-compose exec db pg_dump -U postgres try1_db > ./backups/backup-$(date +%Y%m%d-%H%M%S).sql"
  },
